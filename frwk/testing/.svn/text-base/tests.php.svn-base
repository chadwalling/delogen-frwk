	function getAllChildDirs($dir, &$dirChildren)
	{
		if (is_dir($dir))
		{
			if ($dh = opendir($dir))
			{
				while (($file = readdir($dh)) !== false)
				{
					echo "\nreading dir: $dir";
					if ($file != "." && $file != "..")
					{	
						if (is_dir($dir.$file))
						{
							$dirChildren[]= $dir.$file;
							echo "\ndir: $file";
							getAllChildDirs($dir.$file, $dirChildren);
						}
						else
						{
							echo "\nfile found skipping: $file";
						}
					}
				}
				closedir($dh);
			}
			else
			{
				echo "\ncant open dir: $dir for reading";
			}	
		}
		else
		{
			echo "\nNo child dir in this $dir";
		}
		return $dirChildren;
	}

